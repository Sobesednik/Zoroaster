{
  "name": "zoroaster",
  "version": "4.1.1-alpha",
  "description": "The 2019 Most Modern Testing Framework For Node.JS With Test Contexts (Reusable BeforeEach / AfterEach Via Separate Files); Masks (Inputs/Outputs In Non-Js Files) And Fork Testing; Interactive Snapshots.",
  "main": "depack/index.js",
  "scripts": {
    "t": "node src/bin",
    "test": "yarn t test/spec test/mask -t 10000",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn t test/mask",
    "depack-test": "depack/bin/zoroaster.js test/spec test/mask -t 10000 -a",
    "test-all": "yarn-s test test-build",
    "b": "alamode src/index.js -o depack -s",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "depack": "depack-dev src/bin/zoroaster.js -c -a -p -o depack/bin -s --externs node_modules/@zoroaster/types/externs.js -S",
    "build": "yarn-s d b doc",
    "d": "yarn-s d1",
    "dev": "node src/bin",
    "lint": "eslint .",
    "args": "argufy -o src/bin/get-args.js",
    "wiki": "NODE_DEBUG=doc doc wiki -W ../zoroaster.wiki",
    "example/simple.js": "node src/bin example/simple.js -b",
    "example/Zoroaster/": "node src/bin example/Zoroaster/test/spec -b"
  },
  "files": [
    "depack",
    "src/bin/index.js"
  ],
  "bin": {
    "zoroaster": "depack/bin/zoroaster.js",
    "zoroaster-dev": "src/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/contexttesting/zoroaster.git"
  },
  "keywords": [
    "zoroaster",
    "test",
    "testing",
    "context",
    "tdd",
    "chai",
    "assert",
    "documentation",
    "IDE",
    "suggestions",
    "intellisense",
    "jsdoc",
    "import",
    "export"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/zoroaster/issues"
  },
  "homepage": "https://github.com/artdecocode/zoroaster#readme",
  "dependencies": {
    "@zoroaster/assert": "^1.0.2",
    "@zoroaster/mask": "^2.2.0",
    "alamode": "^2.3.4"
  },
  "devDependencies": {
    "@a-la/fixture-alamode": "1.1.3",
    "@a-la/fixture-babel": "1.2.2",
    "@artdeco/clean-stack": "^1.1.1",
    "@depack/render": "1.3.2",
    "@wrote/exists": "^1.0.5",
    "@wrote/rm": "^1.1.5",
    "@zoroaster/reducer": "^1.3.0",
    "@zoroaster/types": "^1.0.0",
    "argufy": "^1.7.0",
    "catchment": "^3.3.0",
    "documentary": "^1.27.4",
    "erotic": "^2.1.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "makepromise": "^3.1.0",
    "promto": "^1.1.2",
    "reloquent": "^1.3.1",
    "snapshot-context": "2.4.0",
    "spawncommand": "^2.2.0",
    "temp-context": "^2.1.3",
    "usually": "^1.2.0",
    "yarn-s": "1.1.0"
  }
}
